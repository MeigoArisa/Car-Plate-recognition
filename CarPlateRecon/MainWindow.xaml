<Window x:Class="CarPlateRecon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarPlateRecon"
        mc:Ignorable="d"
        Title="MainWindow" Height="864" Width="2010.333">
    <Grid>
        <Button Name="SearchButton" Content="찾아보기" Click="SearchButton_Click" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Image MouseLeftButtonUp="OriginalImage_MouseLeftButtonUp" Name="OriginalImage" HorizontalAlignment="Left" Height="793" Margin="10,30,0,0" VerticalAlignment="Top" Width="900"/>
        <Image MouseLeftButtonUp="ResultImage_MouseLeftButtonUp" Name="ResultImage" HorizontalAlignment="Left" Height="793" Margin="910,30,0,0" VerticalAlignment="Top" Width="900"/>
        <ListBox Name="PictureList" SelectionChanged="PictureList_SelectionChanged" HorizontalAlignment="Left" Height="793" Margin="1815,30,0,0" VerticalAlignment="Top" Width="177"/>

    </Grid>
</Window>
